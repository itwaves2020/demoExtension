{
    "name": "test",
    "description": "test Share ",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_title": "Test",
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background-wrapper.js"
    },
    "content_scripts": [
        {
            "matches": [
                "http://localhost:8000/*"
            ],
            "js": [
                "common/jquery-3.6.0.min.js",
                "js/anywhere.content.js",
                "common/utils.js",
                "common/constants.js",
                "common/session-manager.js"
            ],
            "run_at": "document_end"
        }
    ],
    "host_permissions": [
        "http://localhost:8000/"
    ],
    "permissions": [
        "cookies",
        "sessions",
        "activeTab",
        "tabs",
        "storage"
    ],
    "externally_connectable": {
        "matches": [
            "http://localhost:8000/"
        ]
    }
}