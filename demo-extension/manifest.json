{
    "name": "test",
    "description": "test Share ",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_title": "Test",
        "default_icon": "images/easy-share.png",
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background-wrapper.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://my.setmore.com/*"
            ],
            "js": [
                "js/index.js",
                "common/utils.js",
                "common/constants.js",
                "js/anywhere.content.js"
            ],
            "all_frames": true
        }
    ],
    "host_permissions": [
        "https://my.setmore.com/"
    ],
    "permissions": [
        "cookies",
        "sessions",
        "activeTab",
        "tabs",
        "storage",
        "browsingData"
    ],
    "externally_connectable": {
        "ids": [
            "fohbkncpggaennnegkcbbckpmddjboal"
        ],
        "matches": [
            "https://my.setmore.com/"
        ]
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    }
}