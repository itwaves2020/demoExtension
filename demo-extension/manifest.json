{
    "name": "test",
    "description": "test Share ",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_title": "Test",
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background-wrapper.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "common/utils.js",
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "host_permissions": [
        "http://localhost:8000/"
    ],
    "permissions": [
        "cookies",
        "sessions",
        "activeTab",
        "tabs",
        "storage"
    ]
}