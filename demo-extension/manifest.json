{
    "name": "test",
    "description": "test Share ",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_title": "Test",
        "default_icon": "images/easy-share.png"
    },
    "background": {
        "service_worker": "background-wrapper.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "common/jquery-3.6.0.min.js",
                "js/iframe-content.js",
                "js/content.js",
                "js/index.js",
                "js/anywhere.content.js",
                "common/utils.js",
                "common/constants.js",
                "common/session-manager.js"
            ],
            "all_frames": true
        }
    ],
    "host_permissions": [
        "http://localhost:8000/"
    ],
    "permissions": [
        "cookies",
        "sessions",
        "activeTab",
        "tabs",
        "storage",
        "browsingData"
    ],
    "externally_connectable": {
        "ids": [
            "fohbkncpggaennnegkcbbckpmddjboal"
        ],
        "matches": [
            "http://localhost:8000/"
        ]
    },
    "sandbox": {
        "pages": [
            "index.html",
            "iframe.html"
        ]
    },
    "web_accessible_resources": [
        {
            "resources": [
                "index.html",
                "iframe.html"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}